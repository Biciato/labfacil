<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!--  Font Awesome -->
        <link rel="stylesheet" href="/css/font-awesome.min.css"></link>
        <!--  bootstrap -->
        <link rel="stylesheet" href="/css/bootstrap.min.css"></link>
        <!--  MDB -->
        <link rel="stylesheet" href="/css/mdb.min.css"></link>
        <!--  Custom -->
        <link rel="stylesheet" href="{% block css %}{% endblock %}"></link>
        <link rel="stylesheet" media="(width: 1920px)" href="{% block css19 %}{% endblock %}">
        <link rel="stylesheet" media="(min-width: 1600px) and (max-width: 1700px)" href="{% block css16 %}{% endblock %}">
    </head>
    <body>
      {% block body %}
      {% endblock %}

      <!--  JQuery -->
      <script type="text/javascript" src="/javascript/jquery-3.3.1.min.js"></script>
      <!--  Popper -->
      <script type="text/javascript" src="/javascript/popper.min.js"></script>
      <!--  bootstrap -->
      <script type="text/javascript" src="/javascript/bootstrap.min.js"></script>
      <!--  MDB -->
      <script type="text/javascript" src="/javascript/mdb.min.js"></script>
      <!--  Jquery Mask -->
      <script type="text/javascript" src="/javascript/jquery.mask.min.js"></script>
      <!--  Custom JS -->
      <script type="text/javascript" src="{% block javascript %}{% endblock %}"></script>

    </body>
</html>
